{
  "name": "blu-penguin-backend-assessment",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "migrate": "npx prisma migrate dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "NODE_OPTIONS=--max_old_space_size=4096 npx prisma generate",
    "build": "rm -rf ./dist && tsc && tsc-alias",
    "build:prod": "NODE_OPTIONS=--max_old_space_size=4096 npx prisma generate && rimraf ./dist && tsc && npm prune --omit=dev",
    "start": "npx prisma migrate deploy && node dist/server.js",
    "dev": "ts-node-dev --poll src/server.ts",
    "build:docker": "docker build -t getly-api .",
    "start:docker": "docker run --name getly-container -p 80:8080 -d getly-api",
    "dev:watch": "nodemon --watch src --exec ts-node src/server.ts",
    "cron:test": "jest src/jobs/__tests__/",
    "postinstall": "npm rebuild sharp --force --platform=linux --arch=x64 --libc=glibc && prisma generate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2"
  }
}
